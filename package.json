{
  "name": "onemapsg",
  "version": "0.2.0",
  "author": "Chang Zhe Jiet",
  "description": "Helper library for using OneMap APIs.",
  "keywords": [
    "onemap",
    "map",
    "singapore",
    "sla"
  ],
  "homepage": "https://github.com/ahzhezhe/onemapsg#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahzhezhe/onemapsg"
  },
  "license": "ISC",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint-mac": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "build": "del-cli --force dist && tsc",
    "prepublishOnly": "npm run build",
    "postpublish": "del-cli --force dist",
    "postversion": "git push"
  },
  "dependencies": {
    "fetch-with-proxy": "^1.1.0",
    "form-data": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.25.1"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "test\\.ts$",
    "collectCoverage": true,
    "coverageDirectory": "./test/__coverage__",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test"
    ]
  }
}
